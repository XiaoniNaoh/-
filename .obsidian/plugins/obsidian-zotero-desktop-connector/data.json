{
  "database": "Zotero",
  "noteImportFolder": "",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Format #note",
      "format": "biblatex",
      "cslStyle": "science",
      "command": "autocite"
    }
  ],
  "exportFormats": [
    {
      "name": "Import #note",
      "outputPathTemplate": "{{title}}.md",
      "imageOutputPathTemplate": "{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "templatePath": "Mdnotes 论文笔记模板.md",
      "cslStyle": "chicago-author-date"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "all-imported-notes",
  "shouldConcat": true
}